void setup() {
    size(500, 500);
    background(255);
    draw_grid(4,width);
}    


void draw_grid(int a,int size){
    int i = 1;
    while (i < a*3){
      strokeWeight(1);
      line(size/a*i/3,0,size/a*i/3,size);
      line(0,size/a*i/3,size,size/a*i/3);
      i++;
    }  
    int j = 1;
    while (j < a){
      strokeWeight(2);
      line(size/a*j,0,size/a*j,size);
      line(0,size/a*j,size,size/a*j);
      j++;
    }
}
